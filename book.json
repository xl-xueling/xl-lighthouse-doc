{
  "title": "DTStep",
  "plugins":[
    "expandable-chapters",
    "pageview-count",
    "page-toc-button",
    "insert-logo",
    "multilingual-index"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "associatedWithSummary": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right",
      "multipleH1": false,
      "multipleH2": false,
      "multipleH3": false,
      "multipleH4": false,
      "showLevelIcon": false,
      "showLevel": true,
      "maxLevel": 3,
      "showGoTop":false
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "theme-default": {
      "showLevel": true
    },
    "insert-logo": {
      "url": "/images/logo.png",
      "style": "background: none; max-height: 38px; min-height: 30px"
    }
  }
}